    {
      "path": "dist/batto-utils.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@shopify/jest-koa-mocks": "^3.0.1",
    "@size-limit/preset-small-lib": "^4.11.0",
    "@types/koa": "^2.13.3",
    "husky": "^6.0.0",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^3.9.9"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "koa": "^2.13.1"
  }
